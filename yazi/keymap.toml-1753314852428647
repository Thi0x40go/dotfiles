# ~/.config/fish/config.fish
#Dev Folder
[[manager.prepend_keymap]]
on = ["g", "d" ]
run = "cd $HOME/dev"
desc = "Dev folder"

[[manager.prepend_keymap]]
on   = "<C-d>"
run  = "plugin diff"
desc = "Diff the selected with the hovered file"

[[manager.prepend_keymap]]
on   = [ "c", "a" ]
run  = "plugin compress"
desc = "Archive selected files"
